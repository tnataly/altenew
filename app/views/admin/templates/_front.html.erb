<div class="col-xs-12 col-sm-6 col-md-6">
  <h3>Front</h3>
    <div class="block__edit_template">
      <% if @last_front.present? %>
        <div class="img__edit">
          <%= inline_svg "#{@last_front.image}", class: "front-svg"%>
        </div>
        <%= link_to "Edit", edit_admin_kit_template_front_path(@kit, @template, @last_front), remote: true, id: "btn__orange", class: "btn main__btn main__btn-md main__btn_bg0" %>
        <%= link_to "Delete", admin_kit_template_front_path(@kit, @template, @last_front), method: :delete, data: { confirm: 'Are you sure you want to delete this item?' }, class: "btn main__btn main__btn-md main__btn_bg0" %>
      <% else %>
        <div class="img__edit">
        </div>
        <%= link_to "Add", new_admin_kit_template_front_path(@kit, @template), remote: true, id: "btn__orange", class: "btn main__btn main__btn-md main__btn_bg0" %>
      <% end %>
    </div>
</div>